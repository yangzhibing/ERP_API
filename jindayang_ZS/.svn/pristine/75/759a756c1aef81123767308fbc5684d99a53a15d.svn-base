<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <!-- form 视图-->
        <record id="form_view_afc_solution_item" model="ir.ui.view">
            <field name="name">AFC Solution Item</field>
            <field name="type">form</field>
            <field name="model">afc.solution.item</field>
            <field name="arch" type="xml">
                <form string="故障解决方案">
                    <sheet>
                        <group string="故障解决方案">
                            <group name="group_left">
                                <field name="name"/>
                                <field name="genre"/>
                            </group>
                            <group name="group_right">
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- tree(list)视图 -->
        <record id="tree_view_afc_solution_item" model="ir.ui.view">
            <field name="name">AFC Solution Item</field>
            <field name="type">tree</field>
            <field name="model">afc.solution.item</field>
            <field name="arch" type="xml">
                <tree string="故障解决方案">
                    <field name="name"/>
                    <field name="genre"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>