<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <!-- form 视图-->
        <record id="form_view_afc_query_element" model="ir.ui.view">
            <field name="name">AFC Query Element</field>
            <field name="type">form</field>
            <field name="model">afc.query.element</field>
            <field name="arch" type="xml">
                <form string="查询元素">
                    <sheet>
                        <group string="查询元素信息">
                            <group name="group_left">
                                <field name="id"/>
                                <field name="name"/>
                                <field name="value"/>
                                <field name="type"/>
                            </group>
                            <group name="group_right">
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- tree(list)视图 -->
        <record id="tree_view_afc_query_element" model="ir.ui.view">
            <field name="name">AFC Query Element</field>
            <field name="type">tree</field>
            <field name="model">afc.query.element</field>
            <field name="arch" type="xml">
                <tree string="属性元素">
                    <field name="id"/>
                    <field name="name"/>
                    <field name="value"/>
                    <field name="type"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>